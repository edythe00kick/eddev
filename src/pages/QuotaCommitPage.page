<apex:page standardController="Sales_Numbers__c" extensions="QuotaCommitController" showHeader="false"  sidebar="false">
    
    <apex:form >
       <div style="height:10px;">
       </div>
       <div style="height:30px;background-color:#CCEEFF;text-align:center;padding-top:5px;">
         <span style="font-size:18px; color:#000066;"><b>Quarterly Commit Information</b></span>
       </div>
        <div style="margin:20px 0 0 100px;">
            <span style="font-weight:bold"> Class of Business </span>
            <apex:selectList value="{!oppType}" multiselect="false" size="1"  style="margin-left:5px;">
                <apex:selectOptions value="{!classOfBusiness}"/>
                   <apex:actionSupport event="onchange" action="{!fetchRecords}"  reRender="thePageBlockTable"/>
            </apex:selectList>
            <!-- <apex:outputLink value="https://www.salesforce.com" id="theLink" style="margin-left:500px;">Choose Dates</apex:outputLink> -->
            <apex:commandlink value="Change Quarter" action="{!showPopup}" rerender="popup" style="margin-left:803px;"/>
       
        </div>
        
        
        
        
        
        
        
        
        
        
        
        <apex:outputPanel id="popup">
            <apex:outputPanel styleClass="customPopup" layout="block" rendered="{!displayPopUp}">
                <apex:outputPanel rendered="{!LEN(errStr)!=0}">                        
                    <span style="color:red;"><strong>Error:</strong>&nbsp;{!errStr}</span>
                </apex:outputPanel> 
                <div style="height:10px;"/>
                    
                <div>
                    <span style="font-weight:bold">Enter Quarter</span>
                    <apex:selectList value="{!currMonth}" multiselect="false" size="1"  style="margin-left:5px;">
                        <apex:selectOptions value="{!startmonth}"/>
                           <!--<apex:actionSupport event="onchange" action="{!}" reRender="thePageBlockTable"/> -->
                    </apex:selectList>
                </div>
                <br/>
                <br/>
                <div>
                    <!--<span style="font-weight:bold">Enter End Month</span>-->
                    <apex:selectList value="{!currMonth1}" multiselect="false" size="1"  style="margin-left:84px;padding-right:-20px;">
                        <apex:selectOptions value="{!endmonth}"/>
                        <!-- <apex:actionSupport event="onchange" reRender="xyz"/> -->
                    </apex:selectList>
                </div>  
                <br/>   
                <br/> 
                <div>
                    <apex:commandButton value="Save" action="{!closePopup}" rerender="popup,thePageBlockTable,quarterdiv" style="margin-left:84px;"/>           
                    <apex:commandButton value="Cancel" action="{!cancelPopup}" rerender="popup" style="margin-left:25px;"/>
                </div>
            </apex:outputPanel>
        </apex:outputPanel>
       
     
           <br/>
           <apex:outputPanel id="quarterdiv">
               <table width="100%">
                   <tr>
                       <td width="24%"></td>
                       <td width="23%" align="center"><span style="padding-left: 40px"><b>{!FirstMonthName}</b></span></td>
                       <td width="23%" align="center"><b>{!SecondMonthName}</b></td>
                       <td width="23%" align="center"><b>{!ThirdMonthName}</b></td>
                   </tr>
               </table>
